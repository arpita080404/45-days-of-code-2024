#include <bits/stdc++.h>
using namespace std;

int main() {
     int T;
     cin>>T;
     if(T>=1&&T<=10){
         for(int i=0;i<T;i++){
             int X;
             cin>>X;
             if(X>=1&&X<=9){
                 if (X<=7)
                 cout<<"YES"<<endl;
                 else 
                 cout<<"NO"<<endl;
             }
         }
     }
return 0;
}
