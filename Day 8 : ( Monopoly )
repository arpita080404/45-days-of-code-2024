#include <bits/stdc++.h>
using namespace std;

int main() {
  int T;
  cin>>T;
  if(T>=1&&T<=5000){
      for(int i=0;i<T;i++){
      int P,Q,R,S;
      cin>>P>>Q>>R>>S;
      if(P>=1&&P<=100&&Q>=1&&Q<=100&&R>=1&&R<=100&&S>=1&&S<=100){
         if(P>(Q+R+S)||Q>(P+R+S)||R>(P+Q+S)||S>(P+Q+R))
         cout<<"YES"<<endl;
         else
         cout<<"NO"<<endl;
      }
   }
  }
  return 0;
}
